if [ -f ~/.bashrc ];
then
	source ~/.bashrc
fi

# {{{
# Node Completion - Auto-generated, do not touch.
shopt -s progcomp
for f in $(command ls ~/.node-completion); do
  f="$HOME/.node-completion/$f"
  test -f "$f" && . "$f"
done
# }}}

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/valentinvieriu/Downloads/google-cloud-sdk/path.bash.inc' ]; then source '/Users/valentinvieriu/Downloads/google-cloud-sdk/path.bash.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/Users/valentinvieriu/Downloads/google-cloud-sdk/completion.bash.inc' ]; then source '/Users/valentinvieriu/Downloads/google-cloud-sdk/completion.bash.inc'; fi
